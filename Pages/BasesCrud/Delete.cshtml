@page
@model controleJornadas.Pages.BasesCrud.DeleteModel

@{
    ViewData["Title"] = "Excluir Filial";
}


<h3>Tem certeza que deseja excluir a filial @Model.Bases.Sigla ?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Bases.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Bases.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Bases.Cidade)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Bases.Cidade)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Bases.Sigla)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Bases.Sigla)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Bases.Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-page="./Index">Voltar</a>
    </form>
</div>
